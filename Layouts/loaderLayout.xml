<?xml version="1.0" encoding="utf-8" ?>
<c2layout>
    <name>loaderLayout</name>
    <event-sheet>loaderEvents</event-sheet>
    <size>
        <width>1024</width>
        <height>768</height>
    </size>
    <margins>
        <horizontal>500</horizontal>
        <vertical>500</vertical>
    </margins>
    <unbounded-scrolling>0</unbounded-scrolling>
    <layers>
        <layer name="Layer 0">
            <initially-visible>1</initially-visible>
            <background-color>255,255,255</background-color>
            <transparent>0</transparent>
            <parallax>
                <x>1</x>
                <y>1</y>
            </parallax>
            <zoom-rate>1</zoom-rate>
            <opacity>1</opacity>
            <force-own-texture>0</force-own-texture>
            <instances>
                <instance type="LoadingText">
                    <properties>
                        <text>100%</text>
                        <initial-visibility>Visible</initial-visibility>
                        <font>Lucida Sans Unicode,-64,400,0</font>
                        <color>0,0,0</color>
                        <horizontal-alignment>Center</horizontal-alignment>
                        <hotspot>Top-left</hotspot>
                        <wrapping>Word</wrapping>
                    </properties>
                    <world>
                        <x>406</x>
                        <y>364</y>
                        <z>0</z>
                        <width>200</width>
                        <height>30</height>
                        <depth>0</depth>
                        <hotspotX>0</hotspotX>
                        <hotspotY>0</hotspotY>
                        <angle>0</angle>
                        <opacity>1</opacity>
                    </world>
                </instance>
                <instance type="Sprite">
                    <properties>
                        <initial-visibility>Visible</initial-visibility>
                        <initial-frame>0</initial-frame>
                        <effect>(none)</effect>
                    </properties>
                    <world>
                        <x>499</x>
                        <y>284</y>
                        <z>0</z>
                        <width>97</width>
                        <height>90</height>
                        <depth>0</depth>
                        <hotspotX>0.50515461</hotspotX>
                        <hotspotY>0.5</hotspotY>
                        <angle>0</angle>
                        <opacity>1</opacity>
                    </world>
                </instance>
            </instances>
        </layer>
    </layers>
    <nonworld-instances />
</c2layout>
